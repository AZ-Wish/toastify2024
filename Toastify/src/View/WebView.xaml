<Window x:Class="Toastify.View.WebView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        mc:Ignorable="d"
        Title="Toastify - WebView" Width="570" Height="620" ResizeMode="CanResize" Icon="/Toastify;component/Resources/ToastifyIcon.ico"
        Closing="WebView_OnClosing">
    <Grid>
        <WebBrowser x:Name="WebBrowser" AllowDrop="False" LoadCompleted="WebBrowser_OnLoadCompleted"
                    Navigating="WebBrowser_OnNavigating" Navigated="WebBrowser_OnNavigated" />
    </Grid>
</Window>